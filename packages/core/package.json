{
  "name": "@rn-ui/core",
  "version": "0.0.0",
  "private": false,
  "sideEffects": false,
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "react-native": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --outDir dist --clean",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@react-native/babel-preset": "^0.81.5",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.0",
    "jest": "^29.7.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-test-renderer": "19.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
