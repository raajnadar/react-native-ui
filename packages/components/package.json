{
  "name": "@rn-ui/components",
  "version": "0.0.0",
  "private": false,
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./typography": {
      "types": "./dist/typography/index.d.ts",
      "react-native": "./src/typography/index.ts",
      "default": "./dist/typography/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "typography": [
        "dist/typography/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/typography/index.ts --dts --format cjs --outDir dist --clean"
  },
  "peerDependencies": {
    "@rn-ui/core": "0.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@rn-ui/core": "workspace:*",
    "@types/react": "^19.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
