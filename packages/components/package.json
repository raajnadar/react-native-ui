{
  "name": "@rn-ui/components",
  "version": "0.0.0",
  "private": false,
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./appbar": {
      "types": "./dist/appbar/index.d.ts",
      "react-native": "./src/appbar/index.ts",
      "default": "./dist/appbar/index.js"
    },
    "./button": {
      "types": "./dist/button/index.d.ts",
      "react-native": "./src/button/index.ts",
      "default": "./dist/button/index.js"
    },
    "./icon-button": {
      "types": "./dist/icon-button/index.d.ts",
      "react-native": "./src/icon-button/index.ts",
      "default": "./dist/icon-button/index.js"
    },
    "./layout": {
      "types": "./dist/layout/index.d.ts",
      "react-native": "./src/layout/index.ts",
      "default": "./dist/layout/index.js"
    },
    "./typography": {
      "types": "./dist/typography/index.d.ts",
      "react-native": "./src/typography/index.ts",
      "default": "./dist/typography/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "appbar": [
        "dist/appbar/index.d.ts"
      ],
      "button": [
        "dist/button/index.d.ts"
      ],
      "icon-button": [
        "dist/icon-button/index.d.ts"
      ],
      "layout": [
        "dist/layout/index.d.ts"
      ],
      "typography": [
        "dist/typography/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/appbar/index.ts src/button/index.ts src/icon-button/index.ts src/layout/index.ts src/typography/index.ts --dts --format cjs --outDir dist --clean"
  },
  "peerDependencies": {
    "@expo/vector-icons": ">=15.0.0",
    "@rn-ui/core": "0.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": ">=5.0.0"
  },
  "devDependencies": {
    "@rn-ui/core": "workspace:*",
    "@types/react": "^19.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
