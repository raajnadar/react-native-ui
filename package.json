{
  "name": "react-native-ui",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "turbo run build",
    "lint": "eslint example packages",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "clean": "turbo run clean",
    "clean:cache": "pnpm clean:deep && echo 'âœ¨ All caches cleaned!'",
    "clean:deep": "rm -rf node_modules .turbo .expo dist build && pnpm -r exec rm -rf node_modules dist build .turbo .expo && pnpm store prune && watchman watch-del-all 2>/dev/null || true && rm -rf $TMPDIR/metro-* $TMPDIR/haste-* $TMPDIR/react-* 2>/dev/null || true",
    "example": "pnpm --filter example start",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-react-native": "^4.0.0",
    "prettier": "^3.0.0",
    "typescript-eslint": "^8.0.0",
    "turbo": "^2.0.0"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-native": "0.81.5",
      "react-native-safe-area-context": "5.6.2"
    }
  }
}
